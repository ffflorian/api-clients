{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "dependencies": {
    "@ffflorian/api-client": "2.2.0"
  },
  "description": "A libraries.io API client",
  "devDependencies": {
    "@types/node": "~24",
    "rimraf": "6.1.2",
    "typedoc": "0.28.15",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "engines": {
    "node": ">= 21"
  },
  "exports": "./dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "api",
    "api-client",
    "libraries.io",
    "libraries",
    "librariesio"
  ],
  "license": "GPL-3.0",
  "name": "libraries.io",
  "readme": "https://github.com/ffflorian/api-clients#readme",
  "repository": "https://github.com/ffflorian/api-clients.git",
  "scripts": {
    "build:ts": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc --tsconfig tsconfig.build.json --options ../../typedoc.json --out ../../docs/packages/libraries.io src/index.ts",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build:ts",
    "test": "vitest"
  },
  "version": "3.4.4"
